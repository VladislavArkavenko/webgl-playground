  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --content-base public/ --colors --hot",
    "lint:js": "eslint src/ --ext .js",
    "lint:js:fix": "eslint src/ --ext .js --fix",
    "prettier": "prettier --write src/**/*.js"
  },
  "author": "V.V.Arkavenko",
  "license": "MIT",
  "private": true,
  "description": "WebGl 3D Chart",
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "_husky": {
